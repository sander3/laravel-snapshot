{
    "name": "soved/laravel-snapshot",
    "description": "Full-page browser screenshots",
    "keywords": ["laravel", "snapshot", "soved", "screenshot", "full-page"],
    "type": "library",
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "illuminate/http": "^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "fakerphp/faker": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Soved\\Laravel\\Snapshot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sander de Vos",
            "email": "sander@tutanota.de"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Soved\\Laravel\\Snapshot\\SnapshotServiceProvider"
            ]
        }
    }
}
